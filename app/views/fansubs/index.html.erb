<%= provide(:title, "Fansubs") %>
<div class="row">
  <div class="col-md-12">
    <% @fansubs.each do |fansub| %>
      <div class="fansub">
        <h3><%= fansub.name %></h3>
        <p>
          Homepage: <%= link_to fansub.homepage, fansub.homepage, target: "_blank" %>
        </p>
      </div>
    <% end %>
  </div>
</div>
